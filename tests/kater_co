// coherence, part 3.5 in kater paper

let eco = (co | rf | fr)+
let poloc = po & loc
let co1 = (poloc | rf | fr | co)+ // SC per location
let co2 = po;eco

assume co2 <= 0
assume rfi <= po
//assume rfi <= loc
//assume [W];npo-loc+;fr-imm?;[W] <= co
//assume rf;npo-loc+;[W] <= co
//assume rf;npo-loc+;fr <= co

assert co1 <= 0

